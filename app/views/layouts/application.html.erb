<!DOCTYPE html>
<html>
  <head>
    <title>RorReact</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <% if false %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <% end %>

    <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js" %>
    <%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" %>
    <%= javascript_include_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js' %>
    <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css' %>

    <%= javascript_pack_tag 'rorApp/index' %>
    <%= stylesheet_pack_tag 'rorApp/assets/stylesheets/app' %>


  </head>

  <body>
    <%= yield %>
  </body>
</html>
