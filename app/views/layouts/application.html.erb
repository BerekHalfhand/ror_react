<!DOCTYPE html>
<html>
  <head>
    <title>RorReact</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <% if false %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <% end %>

    <%= javascript_pack_tag 'rorApp/index' %>
    <%= stylesheet_pack_tag 'rorApp/assets/stylesheets/app' %>
    
    <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js" %>

    <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css' %>
    <%= javascript_include_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
